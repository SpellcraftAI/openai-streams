{
  "name": "openai-streams",
  "description": "Tools for working with OpenAI streams in Node.js and TypeScript.",
  "version": "5.1.1",
  "license": "MIT",
  "type": "module",
  "platform": "node",
  "engines": {
    "node": ">=14"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "node.js",
    "node.d.ts"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./node": "./node.js",
    ".": "./dist/index.js",
    "./*": "./dist/*/index.js"
  },
  "scripts": {
    "dev": "tsmodule dev",
    "docs": "pnpm typedoc src",
    "export": "tsmodule build && pnpm docs",
    "test": "pnpm export && ava",
    "retest": "ava",
    "prepublishOnly": "pnpm test",
    "lint": "eslint src --fix"
  },
  "devDependencies": {
    "@tsmodule/tsmodule": "^42.1.0",
    "@types/node": "^18.14.4",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "ava": "^5.2.0",
    "eslint": "^8.35.0",
    "openai": "^3.2.1",
    "typedoc": "^0.23.26",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "eventsource-parser": "^0.1.0",
    "yield-stream": "^3.0.0"
  },
  "keywords": [
    "openai",
    "typescript",
    "streams",
    "gpt-3"
  ],
  "homepage": "https://github.com/SpellcraftAI/openai-streams"
}