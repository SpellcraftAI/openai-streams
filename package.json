{
  "name": "openai-streams",
  "description": "Tools for working with OpenAI streams in Node.js and TypeScript.",
  "version": "5.1.1",
  "license": "MIT",
  "type": "module",
  "platform": "node",
  "engines": {
    "node": ">=14"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "node.js",
    "node.d.ts"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./node": "./node.js",
    ".": "./dist/index.js",
    "./*": "./dist/*/index.js"
  },
  "scripts": {
    "dev": "tsmodule dev",
    "docs": "pnpm typedoc src",
    "export": "tsmodule build",
    "build": "pnpm export && pnpm docs",
    "test": "pnpm export && ava",
    "retest": "ava",
    "prepublishOnly": "pnpm test",
    "lint": "eslint src --fix"
  },
  "devDependencies": {
    "@tsmodule/tsmodule": "^44.5.0",
    "@types/node": "^20.1.1",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "ava": "^5.2.0",
    "eslint": "^8.40.0",
    "openai": "^3.2.1",
    "typedoc": "^0.24.7",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "eventsource-parser": "^1.0.0",
    "yield-stream": "^3.0.0"
  },
  "keywords": [
    "openai",
    "typescript",
    "streams",
    "gpt-3"
  ],
  "homepage": "https://github.com/SpellcraftAI/openai-streams"
}